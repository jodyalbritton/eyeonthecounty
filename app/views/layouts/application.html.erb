	<head>
		<meta charset="utf-8" />
		<title>Eye On The County</title>
		<meta name="keywords" content="henderson, county, news" />
		<meta name="description" content="" />
		<meta name="Author" content="Jody Albritton" />

		<!-- mobile settings -->
		<meta name="viewport" content="width=device-width, maximum-scale=1, initial-scale=1, user-scalable=0" />

		<!-- WEB FONTS -->
		<link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,700,800" rel="stylesheet" type="text/css" />

		<!-- CORE CSS -->
		  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
		 
 
 		 <%= csrf_meta_tags %>
  		 <%= favicon_link_tag 'favicon.ico' %>

	

		<!-- THEME CSS -->
	
	
	
		

		
		

		<!-- Morenizr -->
		 <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	</head>
	<body class="boxed"><!-- Available classes for body: boxed , pattern1...pattern10 . Background Image - example add: data-background="assets/images/boxed_background/1.jpg"  -->

		<!-- Top Bar -->
		<header id="topHead">
			<div class="container">

				<!-- PHONE/EMAIL -->
				<span class="quick-contact pull-left">
					<i class="fa fa-phone"></i> 903-910-9983 &bull; 
					<a href="mailto:info@eyeonthecounty.com">info@eyeonthecounty.com</a>
				</span>
				<!-- /PHONE/EMAIL -->

		

	
			</div>
		</header>
		<!-- /Top Bar -->

		<!-- TOP NAV -->
		<header id="topNav" class="topHead"><!-- remove class="topHead" if no topHead used! -->
			<div class="container">

				<!-- Mobile Menu Button -->
				<button class="btn btn-mobile" data-toggle="collapse" data-target=".nav-main-collapse">
					<i class="fa fa-bars"></i>
				</button>

				<!-- Logo text or image -->
				<%= link_to root_url, :class=> "logo" do %>
					<%= image_tag "logo.png" %>
				<% end %>
			

				<!-- Top Nav -->
				<div class="navbar-collapse nav-main-collapse collapse pull-right">
					<nav class="nav-main mega-menu">
						<ul class="nav nav-pills nav-main scroll-menu" id="topMain">
							<li>
								<%= link_to "Home", root_url %>
							</li>
							<li>
								<%= link_to "About", welcome_about_url %>
							</li>
							<li>
								<%= link_to "Contact", welcome_contact_url %>
							</li>
							<li>
								<%= link_to "Sponsors", sponsors_index_url %>
							</li>
							
							<!-- GLOBAL SEARCH -->
							<li class="search">
								<!-- search form -->
								<form method="get" action="#" class="input-group pull-right">
									<input type="text" class="form-control" name="k" id="k" value="" placeholder="Search">
									<span class="input-group-btn">
										<button class="btn btn-primary notransition"><i class="fa fa-search"></i></button>
									</span>
								</form>
								<!-- /search form -->
							</li>
							<!-- /GLOBAL SEARCH -->

						</ul>
					</nav>
				</div>
				<!-- /Top Nav -->

			</div>
		</header>

		<span id="header_shadow"></span>
		<!-- /TOP NAV -->


		<!-- WRAPPER -->
		<div id="wrapper">
			<%= yield %>
		</div>
		<!-- /WRAPPER -->



		<!-- FOOTER -->
		<footer>

			<!-- copyright , scrollTo Top -->
			<div class="footer-bar">
				<div class="container">
					<span class="copyright">Copyright &copy; Eye On The County . All Rights Reserved.</span>
					<a class="toTop" href="#topNav">BACK TO TOP <i class="fa fa-arrow-circle-up"></i></a>
				</div>
			</div>
			<!-- copyright , scrollTo Top -->


			<!-- footer content -->
			<div class="footer-content">
				<div class="container">

					<div class="row">


						<!-- FOOTER CONTACT INFO -->
						<div class="column col-md-4">
							<h3>CONTACT</h3>

							<p class="contact-desc">
								We are keeping our eye on the county. Let us know if you have a story to share.									
							</p>
							<address class="font-opensans">
								<ul>
									<li class="footer-sprite address">
										1111 Place <br />
										Athens <br />
										Texas, 75701<br />
									</li>
									<li class="footer-sprite phone">
										Phone: 903-910-9983
									</li>
									<li class="footer-sprite email">
										<a href="mailto:support@yourname.com">info@eyeonthecounty.com</a>
									</li>
								</ul>
							</address>

						</div>
						<!-- /FOOTER CONTACT INFO -->


						<!-- FOOTER LOGO -->
						<div class="column logo col-md-4 text-center">
							<div class="logo-content">
								<%= image_tag "logo_footer.png", :class=>"animate_fade_in", :width=>"200" %>
								
								<h4>Eye On The County</h4>
							</div>											
						</div>
						<!-- /FOOTER LOGO -->


						<!-- FOOTER LATEST POSTS -->
						<div class="column col-md-4 text-right">
							

						</div>
						<!-- /FOOTER LATEST POSTS -->

					</div>

				</div>
			</div>
			<!-- footer content -->

		</footer>
		<!-- /FOOTER -->



	


